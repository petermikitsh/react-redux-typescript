{
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "react-hot-loader/babel"
  ],
  "presets": [
    [
      "@babel/preset-env", {
        "modules": false,
        "targets": {
          "browsers": [
            "last 2 versions",
            "IE >= 10"
          ]
        },
        "useBuiltIns": "usage",
        "corejs": 2,
        "debug": true
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ]
}
